{
    "name": "@aquita/api",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "pnpm run prisma:generate && nest build",
        "dev": "nest start --watch",
        "start": "nest start",
        "start:prod": "node dist/main",
        "lint": "eslint \"src/**/*.ts\" \"prisma/**/*.ts\"",
        "test": "vitest run",
        "prisma:migrate": "pnpm exec prisma migrate dev",
