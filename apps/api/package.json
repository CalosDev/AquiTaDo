        "@types/multer": "^2.0.0",
        "@types/node": "^25.3.0",
        "@types/passport-jwt": "^4.0.1",
        "@types/supertest": "^6.0.3",
        "dotenv": "^17.3.1",
        "prisma": "^7.4.1",
        "supertest": "^7.2.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.0"
