        "prisma:backfill:organizations": "pnpm exec ts-node prisma/backfill-organizations.ts",
        "prisma:seed": "pnpm exec ts-node prisma/seed.ts",
        "prisma:studio": "pnpm exec prisma studio"
    },
    "dependencies": {
